{
  "expo": {
    "name": "sociotorcedor",
    "slug": "sociotorcedor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#DF2428"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sociotorcedor.vilanovafc"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#DF2428"
      }, 
      "package": "com.sociotorcedor.vilanovafc"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
